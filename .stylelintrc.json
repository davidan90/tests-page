{
  "processors": [
    "stylelint-processor-html"
  ],
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-sass-guidelines"
  ],
  "rules": {
    "max-nesting-depth": 5,
    "selector-max-compound-selectors": 10,
    "selector-max-id": 1,
    "selector-no-qualifying-type": null,
    "string-quotes": "double"
  }
}
